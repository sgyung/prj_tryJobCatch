   $('ul li button').on('click', function () {
	    // 현재 클릭된 버튼의 부모 li 요소를 선택
	    const $parentLi = $(this).closest('li');

	    // li 요소의 클래스를 가져오기
	    const targetClass = $parentLi.attr('class');

	    // 클래스에서 'BOX'를 제거하여 목표 ID 추출
	    const targetId = targetClass.replace(/\s*BOX$/, '');

	    // ID를 이용하여 체크박스를 찾아서 해제
	    $('#' + targetId).prop('checked', false);

	    // 클릭한 li 요소의 display 속성을 'none'으로 변경
	    $parentLi.css('display', 'none');
	});
   

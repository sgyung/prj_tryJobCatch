	 // 체크박스 상태 변경 시
   $(".chkkwrd").on('change', function () {
	    const targetId = $(this).attr('id');
	    const targetElement = $('.' + targetId);
	
	    
	    if ($(this).prop('checked')) {
	        targetElement.css('display', 'inline-block'); // Show when checked
	    } else {
	        targetElement.css('display', 'none'); // Hide when unchecked
	    }
